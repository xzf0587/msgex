{
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "TextBlock",
      "size": "Medium",
      "weight": "Bolder",
      "text": "${title}"
    },
    {
      "type": "Container",
      "items": [
        {
          "type": "TextBlock",
          "text": "List of Peers"
        },
        {
          "$data": "${participators}",
          "type": "TextBlock",
          "size": "Medium",
          "weight": "Bolder",
          "text": "${string($index + 1)}: ${name}"
        }
      ]
    }
  ],
  "actions": [
    {
      "type": "Action.Execute",
      "title": "Join!",
      "verb": "userlike",
      "fallback": "Action.Submit"
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.4"
}